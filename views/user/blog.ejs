<!-- content -->
<div class="content">
                        <section>
                            <div class="container">
                                <div class="row">
                                    <!-- Articels -->
                                    <div class="col-md-8">
                                        <article>
                                            <h2 class="section-title dec-title"><span>Create A Blog </span></h2>
                                            <form ng-submit="submitblogForm()" id="blog-form" class="form-blog" name="blog">
                                                <!-- T I T L E -->
                                                <div class="col-md-12"><label>Title</label></div>
                                                <div class="control-group form-group col-md-12" ng-class="{'has-error':blog.name.$invalid && blog.name.$dirty}">
                                                <input type="text" class="form-control" placeholder="e.g. Nikola Tesla" name="title" ng-model="blogForm.title" ng-maxlength="50" required>
                                                    <span class="help-block has-error" ng-if="blog.name.$dirty">
                                                    <span ng-show="blog.name.$error.required">Name is required.</span>
                                                    <span ng-show="blog.name.$error.maxlength">The name cannot be more than 50 characters.</span>
                                                    </span>
                                                </div>
                                                <textarea name="description" cols="97" rows="10"></textarea>
                                                <br>
                                                Select image to upload:
                                                <input type="file" name="fileToUpload" id="fileToUpload">
                                                <button id="button1" class="btn btn-primary btn-lg btn-block" type="submit" ng-disabled="blog.$invalid">
                                                    <span ng-show="!blogForm.loading">Submit Blog</span>
                                                    <span class="overlord-loading-spinner fa fa-spinner" ng-show="blogForm.loading" ></span>
                                                    <span ng-show="blogForm.loading">Sending blog to Admin</span>
                                                </button>

                                                <input type="hidden" name="_csrf" value="<%= _csrf %>" />
                                            </form>
                                        </article>
                                        
                                        <div class="pagination-blog">
                                            <a href="#" class="prevposts-link transition"></a>
